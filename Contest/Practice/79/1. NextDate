using System;

class NextDate_1
{
    static void Main()
    {
        int day = int.Parse(Console.ReadLine());
        int month = int.Parse(Console.ReadLine());
        int year = int.Parse(Console.ReadLine());
        DateTime inputDate = new DateTime(year, month, day).AddDays(1);
        Console.WriteLine("{0:d.M.yyyy}", inputDate);
    }
}
